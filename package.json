{
  "name": "frps-auth-dashboard",
  "version": "0.33.0",
  "description": "一款基于nodejs的frps的服务端插件，此插件随frp更新同步更新，主要用于客户端授权管理等功能",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "egg": "^2.15.1",
    "egg-cors": "^2.2.3",
    "egg-jwt": "^3.1.7",
    "egg-redis": "^2.4.0",
    "egg-scripts": "^2.11.0",
    "egg-sequelize": "^5.2.1",
    "egg-valparams": "^1.4.2",
    "mysql2": "^2.1.0",
    "sqlite3": "^4.2.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0",
    "sequelize-cli": "^5.5.1"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "migrate": "npx sequelize db:migrate",
    "start": "egg-scripts start --daemon --title=egg-server-example",
    "stop": "egg-scripts stop --title=egg-server-example",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Shijf/fp-auth"
  },
  "author": "shijf",
  "license": "MIT"
}
